  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "macos": "react-native run-macos",
    "windows": "react-native run-windows",
    "web": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "build:preload": "node electron/build-preload.js",
    "electron-dev": "npm run build:preload && cross-env NODE_ENV=development concurrently \"npm run web\" \"wait-on http://localhost:3000 && electron .\"",
